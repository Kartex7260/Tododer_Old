<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ProgressView">
        <attr name="ratio" format="float" />
        <attr name="strokeColor" format="color" />
        <attr name="fillColor" format="color" />
    </declare-styleable>

    <declare-styleable name="TodoDetailView">
        <attr name="title" format="string" />
    </declare-styleable>

    <declare-styleable name="RemarkTodoDetailView">
        <attr name="remark" format="string" />
    </declare-styleable>

    <declare-styleable name="TreeTodoDetailView">
        <attr name="todoListFilter" >
            <flag name="byTitle" value="0" />
            <flag name="byTime" value="1" />
        </attr>
        <attr name="sortReverse" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TaskDetailView">
        <attr name="check" format="boolean" />
    </declare-styleable>

</resources>